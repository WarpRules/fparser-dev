T=d ld li f mf gi
V=x,y
R=0,1,1
F=  1*if(x<y,x,y) + \
    2*if(x<=y,x,y) + \
    4*if(x>=y,x,y) + \
    8*if(x>y,x,y) + \
 0x10*if(x<y,y,x) + \
 0x20*if(x<=y,y,x) + \
 0x40*if(x>=y,y,x) + \
 0x80*if(x>y,y,x) + \
 0x0100*if(y<x,x,y) + \
 0x0200*if(y<=x,x,y) + \
 0x0400*if(y>=x,x,y) + \
 0x0800*if(y>x,x,y) + \
 0x1000*if(y<x,y,x) + \
 0x2000*if(y<=x,y,x) + \
 0x4000*if(y>=x,y,x) + \
 0x8000*if(y>x,y,x)
C=  1*(x<y?x:y) + \
    2*(x<=y?x:y) + \
    4*(x>=y?x:y) + \
    8*(x>y?x:y) + \
 0x10*(x<y?y:x) + \
 0x20*(x<=y?y:x) + \
 0x40*(x>=y?y:x) + \
 0x80*(x>y?y:x) + \
 0x0100*(y<x?x:y) + \
 0x0200*(y<=x?x:y) + \
 0x0400*(y>=x?x:y) + \
 0x0800*(y>x?x:y) + \
 0x1000*(y<x?y:x) + \
 0x2000*(y<=x?y:x) + \
 0x4000*(y>=x?y:x) + \
 0x8000*(y>x?y:x)

