T=d ld li f mf gi
V=x,y
R=-6,6,1
F= 0x0001 * ((x*-4*y) < 6) + \
   0x0002 * ((x*-4*y)<= 6) + \
   0x0004 * ((x*-4*y)!= 6) + \
   0x0008 * ((x*-4*y) = 6) + \
   0x0010 * ((x*-4*y)>= 6) + \
   0x0020 * ((x*-4*y) > 6) + \
   0x0100 * ((x*-4) < (y*6)) + \
   0x0200 * ((x*-4)<= (y*6)) + \
   0x0400 * ((x*-4)!= (y*6)) + \
   0x0800 * ((x*-4) = (y*6)) + \
   0x1000 * ((x*-4)>= (y*6)) + \
   0x2000 * ((x*-4) > (y*6))
C= 0x0001 * ((x*-4*y) < 6) + \
   0x0002 * ((x*-4*y)<= 6) + \
   0x0004 * ((x*-4*y)!= 6) + \
   0x0008 * ((x*-4*y)== 6) + \
   0x0010 * ((x*-4*y)>= 6) + \
   0x0020 * ((x*-4*y) > 6) + \
   0x0100 * ((x*-4) < (y*6)) + \
   0x0200 * ((x*-4)<= (y*6)) + \
   0x0400 * ((x*-4)!= (y*6)) + \
   0x0800 * ((x*-4)== (y*6)) + \
   0x1000 * ((x*-4)>= (y*6)) + \
