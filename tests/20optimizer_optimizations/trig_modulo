T=d ld f mf cd cf cld
V=x
# Step = -3piâ€¦+3pi, step pi/4
R=-3,3,0.25
F=y := x*pi; p23 := pi*2/3; p52 := pi*5/2; p62 := pi*6/2; p72 := pi*7/2; \
  cos(y+pi) + cos(y+p23) + cos(y+p52) + cos(y+p62) + cos(y+p72) + \
  sin(y+pi) + sin(y+p23) + sin(y+p52) + sin(y+p62) + sin(y+p72) + \
  cos(y-pi) + cos(y-p23) + cos(y-p52) + cos(y-p62) + cos(y-p72) + \
  sin(y-pi) + sin(y-p23) + sin(y-p52) + sin(y-p62) + sin(y-p72)

